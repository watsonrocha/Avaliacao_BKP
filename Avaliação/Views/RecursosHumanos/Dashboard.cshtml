    @{
        foreach (var item in (List<RecursosHumanosModel>)ViewBag.RetornaNine)
        {
            <head>
                <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
                <script type="text/javascript">
        google.charts.load('current', { 'packages': ['corechart'] });
        google.charts.setOnLoadCallback(drawSeriesChart);

        function drawSeriesChart() {

            var data = google.visualization.arrayToDataTable([
                ['Colaborador', 'X', 'Y','Unidade'],
                ['@item.Nome',  (@item.Total_Competencias.ToString()), 50, 'Sorocaba'],




            ]);

            var options = {
                title: 'TK Logistica do Brasil ',
                hAxis: { title: 'TK' },
                vAxis: { title: 'NineBox' },


                bubble: { textStyle: { fontSize: 11 } }
            };

            var chart = new google.visualization.BubbleChart(document.getElementById('series_chart_div'));
            chart.draw(data, options);
        }
                </script>
            </head>
            <body>
                <div id="series_chart_div" style="width: 100%; height: 100%;"></div>
            </body>
            }
        }
        </html>
